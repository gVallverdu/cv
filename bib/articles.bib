	number = {16},
	urldate = {2021-05-05},
	journal = {The Journal of Physical Chemistry C},
	author = {Lemoine, Alexia and Invernizzi, Ronan and Salvato Vallverdu, Germain and Madec, Lénaïc and Olchowka, Jacob and Guerlou-Demourgues, Liliane and Baraille, Isabelle and Flahaut, Delphine},
	month = apr,
