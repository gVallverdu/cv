	note = {Publisher: American Chemical Society},
	pages = {8570--8581},
	file = {Full Text PDF:/Users/gvallver/Zotero/storage/HGGXPSJM/Lemoine et al. - 2021 - Surface Reactivity and Surface Characterization of.pdf:application/pdf},
}

